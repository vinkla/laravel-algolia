{
    "name": "vinkla/algolia",
    "description": "An Algolia bridge for Laravel",
    "keywords": [
        "algolia",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "homepage": "https://github.com/vinkla"
        }
    ],
    "require": {
        "php": "^7.4",
        "algolia/algoliasearch-client-php": "^2.5",
        "graham-campbell/manager": "^4.4",
        "illuminate/contracts": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.0",
        "graham-campbell/testbench": "^5.4",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Algolia\\AlgoliaServiceProvider"
            ],
            "aliases": {
                "Algolia": "Vinkla\\Algolia\\Facades\\Algolia"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Algolia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Algolia\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
